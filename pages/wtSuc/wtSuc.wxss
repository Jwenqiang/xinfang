.msg{padding: 0 40rpx;box-sizing: border-box;font-size: 28rpx;border-bottom: 1px solid #ededed}
.msg label{width: 200rpx;display: inline-block;float: left;height: 100rpx;line-height: 100rpx;}
.msg input{width: 450rpx;height: 100rpx;line-height: 100rpx;float: left;text-align: right}
.uploadAll{padding: 40rpx;font-size: 28rpx}
.title{margin-bottom: 40rpx;width: 100%}
.upload label{display: block;width: 140rpx;height: 140rpx;border: 1px dashed #ccc;text-align: center;border-radius: 2px;position: relative;box-sizing: border-box}
.upload image{width: 100%; height: 140rpx}
.upload label.imgS{border: 0;}
.upload .iconfont{position: absolute;right: 0rpx;top: 0rpx;width: 28rpx;height: 28rpx;line-height: 1;font-size: 28rpx;z-index: 9}
.top{background: #f5f5f7;padding: 80rpx 0 40rpx}
.tip{text-align: center;margin-bottom: 80rpx;font-size: 28rpx;}
.tipMsg{font-weight: bold;margin-bottom: 20rpx}
.tip icon{margin-bottom: 40rpx}
.tip text{display: block}
.btn {margin-top: 40rpx;text-align: center}
.btn button{width: 320rpx;margin-bottom: 20rpx;background: #5f92ff;box-sizing: border-box;color: #fff}
.btn button:last-child{border: 1px solid #5f92ff ;background: none;color:#5f92ff }