@import "../font.wxss";


.searchM {
  width: 670rpx;
  height: 100rpx;
  position: fixed;
  padding: 20rpx 0;
  top: 0;
  left: 40rpx;
  background: #fff
}

.searchM input {
  width: 570rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 20px;
  background: #fff;
  font-size: 24rpx;
  padding-left: 80rpx;
  box-sizing: border-box;
  border: 1px solid #ededed;
  float: left;
}
.searchM .icon-sousuo{
  position: absolute;
  top:38rpx;
  left: 26rpx;
  color: #ccc
}
.cancel{display: block;width: 100rpx;height: 70rpx;float: left;text-align: right;line-height: 70rpx;}
.allData{width: 100%;padding-top: 100rpx}
.allData .dList{padding: 20rpx;border-bottom: 1px solid #f5f5f7}
.allData .dList label{display: block;margin-bottom: 10rpx;width: 100%;font-weight: bold}
.allData .dList text{font-size: 22rpx;color: #999}
.sTitle{margin-bottom: 40rpx}
.sRecord text{display: inline-block;margin: 0 10rpx 20rpx;padding: 6rpx 10rpx;border: 1px solid #ddd;border-radius: 3px}