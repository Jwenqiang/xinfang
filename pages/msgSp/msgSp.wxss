page{background: #f5f5f7;font-size: 28rpx;}
.msgSp{padding: 40rpx;}
.section{margin-bottom: 40rpx;}
.sTime{
  text-align: center;
    line-height: 80rpx;
    font-size: 28rpx;  
    color: #999
}
.secMsg{
  background: #fff;
    padding: 40rpx; 
    box-sizing: border-box 
}
.msTitle{font-size: 24rpx;font-weight: bold;padding-bottom: 32rpx;border-bottom: 1px solid #ededed}
.house{padding: 40rpx 0 0;box-sizing: border-box;}
.house image{width: 200rpx;height: 140rpx;float: left;margin-right: 40rpx;border-radius: 3px}
.hifMsg .houseTitle{
    -webkit-line-clamp: 2;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow: hidden;  
}
.goFind{width: 240rpx;height: 80rpx;margin-top: 40rpx;background: #3162fd;color: #fff;font-weight: bold;border-radius: 20px;font-size: 28rpx;}
