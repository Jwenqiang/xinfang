@import "../font.wxss";
page,.page{width: 100%; height: 100%; position: relative;padding-bottom: 100rpx}
.topImg, .topImg image {
  width: 100%;
  height: 300rpx;
}
.red{color: #fe483a;font-size: 28rpx}
.search {
  width: 750rpx;
  height: 90rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  position: relative;
  top:-45rpx;

}
.fixSearch{
  top:0;
  margin-bottom: 20rpx;
}

.mTop{height: 300rpx}
.search input {
  width: 670rpx;
  height: 90rpx;
  line-height: 90rpx;
  box-shadow: 0 10rpx 20rpx #f0f2f7;
  border-radius: 3px;
  background: #fff;
  font-size: 28rpx;
  padding-left: 80rpx;
  box-sizing: border-box
}
.fixSearch input{
  box-shadow: none;
  border: 1px solid #ededed;
  height: 80rpx
}
.search .icon-sousuo{
  position: absolute;
  top:20rpx;
  left: 60rpx;
  color: #ccc;
  font-size: 42rpx;
}
.fixSearch .icon-sousuo{
  top: 17rpx
}
.show-s{width: 100%;height: 100%;padding: 20rpx 40rpx;background: #fff;box-sizing: border-box;z-index: 88;position: fixed;top: 0;left: 750rpx}
.show{left: 0}
.searchM {
  width: 670rpx;
  height: 80rpx;
  position: relative;
  font-size: 28rpx
}

.searchM input {
  width: 570rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 20px;
  background: #fff;
  font-size: 24rpx;
  padding-left: 80rpx;
  box-sizing: border-box;
  border: 1px solid #ededed;
  float: left;
}
.searchM icon{
  position: absolute;
  top:20rpx;
  left: 20rpx
}
.cancel{display: block;width: 100rpx;height: 70rpx;float: left;text-align: right;line-height: 70rpx;}

/* .mContent{padding: 40rpx 0rpx 0;} */
.mcT{font-size: 44rpx;margin-bottom: 56rpx;}
.hli{height: 72rpx;}
.hli view{width: 28%;float: left;font-size: 28rpx;text-align: center;line-height: 60rpx}
/* .hli view label{display:inline-block;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;width: 100rpx;} */
.hli view text{font-size: 24rpx;margin-left: 8rpx;vertical-align: middle}

.hli-qy{width: 100%;}
.qyL,.qyR{width: 33.3%;float: left;text-align: center;height: 328rpx;overflow: auto}
.qyL{background: #f5f5f7}
.qyL view,.qyR view{height: 80rpx;line-height: 80rpx;border-bottom: 1px solid #f5f5f7;}
.qyR view{border-right: 1px solid #f5f5f7}
.qyL view.on{background: #fff}
.qyR view.on{color: #fe7269}
.hli-one view{width: 100%;height: 80rpx;line-height: 80rpx;text-align: center;}
.hli-one view.on{color: #fe7269}


.hliC{width: 100%;height: 400rpx;box-shadow: 0 0 5px #f5f6f5}
.hliC view{font-size: 28rpx;width: 100%;text-align: center;line-height: 80rpx;}
.mhTag{width: 750rpx;padding: 0;box-sizing: border-box;background: #fff !important;position: relative;}
.hli-show{position: fixed;left: 0;top: 146rpx;background: #fff;width: 100%;box-sizing: border-box; max-height:100%; z-index: 100;box-shadow: 0 3px 5px #ededed}
.listData{height: 100%}
.hli-all{padding: 40rpx;width: 100%;box-sizing: border-box;transition: all 1s ease}
.all-msg{width: 100%;box-sizing: border-box}
.all-msg label{
    display: inline-block;
    width: 149rpx;
    height: 60rpx;
    line-height: 60rpx;
    border: 1px solid #ededed;
    background: #ededed;
    border-radius: 4rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
    font-size: 24rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    color: #999;
    text-align: center
}
.all-content{height: 1000rpx;overflow-y: auto;}
.all-sub{text-align: center;position: fixed;bottom:0;left: 0;width: 100%;background: #fff;height: 140rpx;padding-top: 40rpx;box-shadow: 0 -3px 5px #f5f5f7}
.all-sub button{width: 240rpx; height:80rpx;margin: 0 40rpx;display: inline-block;border: 1px solid #ccc;color: #ccc;border-radius: 50rpx;background: none;line-height: 76rpx;text-align: center}
.all-sub button:last-child{background: #fe7269;border: 1px solid #fe7269;color: #fff}
.all-msg label:nth-of-type(4n){margin-right: 0}
.all-msg label.on{background: #fe7269;color: #fff;border: 1px solid #fe7269}
.all-section{margin-bottom: 0.4rem}
.all-title{margin-bottom: 0.8rem}
.houseL{padding: 0 40rpx;margin-bottom: 6rpx}
.tagList{border-bottom: 1px solid #f5f5f7;padding: 0 40rpx 10rpx}
.mhTag .tagInfo{display: inline-block;width: 114rpx;height: 40rpx; border: 1px solid #f5f5f7; line-height: 40rpx;font-size: 20rpx;background: #f5f5f7;border-radius: 20rpx;text-align: center;margin-left: 20rpx; margin-bottom: 20rpx; color: #999}
.mhTag .tagInfo:first-child{margin-left: 0}
.mhTag .tagInfo.on{border: 1px solid #fe7269;background: #fe7269;color: #fff}
.showBj{position: fixed;top: 0;left: 0;height: 100%;width: 100%;background: #000;opacity: 0.5;z-index: 99}

.thTop{position: relative}
.ztlist{margin-top: 30rpx;margin-left: 40rpx; padding-bottom: 40rpx;border-bottom: 1px solid #f5f5f7;box-sizing: border-box}
.mContent .ztlist:last-child{border-bottom:0;}
.topFix{position: absolute;width: 80rpx;height: 36rpx;line-height: 34rpx;text-align: center; background: #000;color: #fff;font-size: 16rpx;left: 0;top: 10rpx;border-top-right-radius: 28rpx;border-bottom-right-radius: 28rpx;}
.ztlist image{width:100%;height: 300rpx;border-radius: 3px}
.thTop label,.thTop label image{width: 44rpx;height: 54rpx;float: left;margin-right: 30rpx;text-align: center;line-height: 54rpx;font-weight: bold;font-size: 32rpx;}
image.hmsg{width: 200rpx;height: 154rpx;float: left;margin-right: 30rpx;margin-top: 10rpx}
.bigT{font-size: 28rpx;font-weight: bold;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;padding-right: 40rpx;box-sizing: border-box}
.smallT{float: left;color: #999;line-height: 1;font-size: 22rpx;width: 450rpx;margin-bottom: 4rpx}
.smallT text.price{font-size: 32rpx;color: #fe483a;font-weight: 600;}
.smallT .label{margin-left: 20rpx;text-decoration: line-through;font-size: 22rpx;color: #999;}
.thBot{margin-top: 40rpx;}
.maxT{margin-bottom: 20rpx;font-size: 38rpx;font-weight: bold;}
.hinfo{font-size: 22rpx;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;color: #999;margin-bottom: 4rpx}
.signT{font-size: 20rpx;color: #999}
.tag{width: 100%;font-size: 24rpx;}
.tag text{display: inline-block;width: 80rpx;height: 36rpx;line-height: 36rpx;font-size: 16rpx;background: #ebf3f8;border-radius: 3px;text-align: center;color: #999;margin-top: 12rpx;margin-left: 12rpx;font-weight: bold}
.tag text:first-child{margin-left: 0}
.zu{margin-top: 14rpx}
.zu,.jun{width: 450rpx;float:left;margin-left: 230rpx; font-size: 23rpx;height: 50rpx;line-height: 50rpx}
.zu .label{background: #f67b6f;font-size: 20rpx;width: 28rpx;height: 28rpx;line-height: 28rpx;color: #fff;border-radius: 3px;margin-right: 10rpx;margin-top: 9rpx}
.jun .label{background: #51bbdd;font-size: 20rpx;width: 28rpx;height: 28rpx;line-height: 28rpx;color: #fff;border-radius: 3px;margin-right: 10rpx;margin-top: 9rpx}
.jun text{color: #f67b6f}
.jun .iconfont{font-size: 20rpx}

.mhSun{font-size: 22rpx;width: 480rpx;height: 40rpx;line-height: 40rpx;float: right;color: #33cccc;margin-top: 20rpx}
.mhSun label{font-size: 22rpx;height: 40rpx;line-height: 40rpx; width: 70rpx;text-align: left;margin-right: 0;}
/* .mhSun text{float: right;} */
.mhSun progress{width: 250rpx;margin-top: 16rpx;float: left;margin-right: 20rpx}